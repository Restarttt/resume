  background-color: #f3f4f5;
}

.logo {
  width: 100%;
  height: 500rpx;
  position: relative;
}

.logo image {
  width: 100%;
  height: 100%;
}

.logo_title {
  position: absolute;
  top: 28%;
  left: 34%;
  text-align: center;
}

.name {
  font-size: 58rpx;
  color: #Fff;
  font-weight: 600;
}

.line {
  width: 100%;
