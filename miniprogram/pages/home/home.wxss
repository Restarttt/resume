  color: #323131;
  font-size: 30rpx;
  width: 84%;
}
.interface{
  position:relative;
  width: 55%;
  height:70rpx;
  border: 2rpx solid #c7c7c7 ;
  background-color: #f7f0f0;
  border-radius: 5rpx;
  text-align: center;
  line-height: 70rpx;
  left: 19%;
  font-size: 500;
  font-size: 38rpx;
  margin: 30rpx;
  padding: 10rpx 0;
}
.cut-off{
