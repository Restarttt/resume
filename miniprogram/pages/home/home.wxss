  position: relative;
  left: 43rpx;
  top: 4rpx
}

.title_text {
  color: #fff;
  font-size: 38rpx;
  font-weight: 500;
}

.content {
  background-color: #fff;
  border-radius: 15rpx;
  padding: 30rpx 25rpx;
  z-index: 1111;
  box-shadow: 0px 1px 4px rgba(0, 0, 0, 0.3),
    0px 0px 20px rgba(0, 0, 0, 0.1) inset;
   position: relative;
}

.content_frist {
  margin: 30rpx 0;
